object LeapTool {
  /// \internal
  Data handle;
};

/// \internal
function ~LeapTool() = "LeapTool_Destruct";

/// returns true if this controller is valid
function Boolean LeapTool.valid() {
  Data nullData;
  return this.handle == nullData;
}

